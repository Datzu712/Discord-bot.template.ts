{
  "name": "template-discord-bot.git",
  "version": "0.0.1",
  "description": "A template of a Discord bot made in TypeScript.",
  "main": "src/index.ts",
  "scripts": {
    "eslint": "eslint 'src/**/*.ts'",
    "eslint:fix": "eslint --fix 'src/**/*.ts'",
    "prettier": "prettier 'src/**/*.ts'",
    "prettier:fix": "prettier --write 'src/**/*.ts'",
    "build": "tsc -p .",
    "dev": "echo Shards disabled. If you want use shards, run the bot without dev script. && nodemon src/index.ts",
    "build:start": "npm run build && npm start",
    "start": "node dist/shards.js",
    "start:noshards": "node dist/index.js",
    "check": "npm run eslint && npm run prettier"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Datzu712/Discord_bot-TS.git"
  },
  "author": "Datzu712",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Datzu712/Discord_bot-TS/issues"
  },
  "homepage": "https://github.com/Datzu712/Discord_bot-TS#readme",
  "dependencies": {
    "discord.js": "^13.8.0",
    "dotenv": "^16.0.1",
    "fs-extra": "^10.1.0",
    "i18next": "^21.8.9",
    "js-beautify": "^1.14.3",
    "moment": "^2.29.3",
    "mongoose": "^6.3.6",
    "npm": "^8.12.1",
    "sourcebin": "^4.3.5",
    "string-similarity": "^4.0.4",
    "typescript": "^4.7.3"
  },
  "devDependencies": {
    "@sentry/node": "^7.1.1",
    "@types/fs-extra": "^9.0.13",
    "@types/js-beautify": "^1.13.3",
    "@types/string-similarity": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-security": "^1.5.0",
    "prettier": "^2.6.2",
    "ts-node": "^10.8.1", 
    "eslint": "^8.17.0"
  }
}
